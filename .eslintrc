{
    "env": {
        "node": true,
        "es2022": true,
        "browser": true
    },
    "plugins": ["prettier"],
    "extends": ["@nuxt/eslint-config"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}